
public class calculator {
	private int n1;
	private int n2;
	private String op;
	
	public long calc() {
		long result = 0;

		switch(op) {
			case "+": 
				result = n1+n2;
				break;
			case "-": 
				result = n1-n2;
				break;
			case "/": 
				result = n1/n2;
				break;
			case "*": 
				result = n1*n2;
				break;
			default:
				result = 0;
		}
		
		return result;
	}
}
