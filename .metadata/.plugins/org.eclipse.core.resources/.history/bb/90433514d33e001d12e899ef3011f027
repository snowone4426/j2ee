<%@ page import="java.text.SimpleDateFormat" %>
<%
Date now = new Date();
SimpleDateFormat df = new SimpleDateFormat("yyyy-MM-dd");
String date = df.format(now);
if(date.endsWish("12")){
	response.sendRedirect("underCheck.jsp");
}
%>