package jdbc;

import java.sql.*;
import utill.DBUtil;

public class JDBCstudy {
	Statement stmt;
	
	JDBCstudy () {
		try {
			ResultSet rs = null;
			stmt = DBUtil.getConnection("scott","tiger").createStatement();
			rs = stmt.executeQuery("select * from emp");
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	
}
