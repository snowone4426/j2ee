package jdbc;

import java.sql.*;
import utill.DBUtil;
import java.util.Vector;

public class JDBCstudy {
	Statement stmt = null;
	Vector<String> list = new Vector<>();
	
	public JDBCstudy () {
		ResultSet rs = null;
		try {
			String[] keyArr = {"empno","ename"};
			stmt = DBUtil.getConnection("scott","tiger").createStatement();
			rs = stmt.executeQuery("select * from emp");
			
			while (rs.next()) {
				for (String key : keyArr) {
					list.add(key + ": " + rs.getString(key) + "   ");
				}
			}
			
		} catch (SQLException e) {
			e.printStackTrace();
		} finally {
			try {
				rs.close();
				stmt.close();
				DBUtil.close();
				
			} catch (Exception e2) {
				e2.printStackTrace();
			}
		}
	}
	
	public Vector<String> getList () {
		return list;
	}
	
}
