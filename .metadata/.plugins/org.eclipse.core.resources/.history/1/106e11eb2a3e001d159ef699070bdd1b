package dao;

import java.util.ArrayList;
import dto.Product;


public class ProductRepasitory {
	private ArrayList<Product> list = new ArrayList<>();
	
	public ProductRepasitory () {
		Product phone = new Product("P1234","iPhone 14", 130000);
		phone.setDescription(null);
		phone.setCategory(null);
		phone.setCondition(null);
		phone.setManufacturer(null);
		phone.setUnitsInstock(0);
	}
}
